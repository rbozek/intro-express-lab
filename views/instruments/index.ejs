<body>
 <h1>Instruments!</h1>
 <ul>
   <% instruments.forEach(instrument => { %>
     <li>
   <%= instrument.text %> 
   - 
   <%= instrument.loud ? 'loud' : 'not so loud' %>
   - 
   <%= instrument.easytocarry ? 'easy to carry' : 'heavy as hell' %>
  </li>
   <% }) %>
 </ul>
<h2><%= name %></h2>
<h2><%= otherthing %></h2>
</body>

